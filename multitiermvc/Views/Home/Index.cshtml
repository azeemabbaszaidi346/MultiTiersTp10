@model IEnumerable<CommanLayer.Models.Employees>
@{
}
<button class="btn btn-block"><a asp-action="Create" asp-controller="Home" >Create Employee</a></button>
<table class=" table table-bordered"\>
    <thead>
        <tr>
            <th> Id</th>
            <th> Name</th>
            <th> Email</th>
            <th> Phone</th>
            <th> Gender</th>
            <th> Action</th>
        </tr>
    </thead>
    @foreach (var emp in Model)
    {
     <tr>
    <td> @emp.Id</td>
    <td> @emp.Name</td>
    <td> @emp.Email</td>
    <td> @emp.Phone</td>
    <td> @emp.Gender</td>
    <td> <a asp-action="Update" asp-controller="Home" asp-route-id="@emp.Id">Edit</a>||
    <a asp-action="Delete" asp-controller="Home" asp-route-id="@emp.Id">Delete</a></td>
</tr>
    }
</table>